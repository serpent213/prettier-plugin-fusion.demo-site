{
  "name": "astro-website-list-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && find dist -name '*.html' -exec sed -i 's|href=\"/|href=\"./|g; s|src=\"/|src=\"./|g' {} +",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check": "npm run format && npm run lint && npm run astro check",
    "fix": "npm run format:fix && npm run lint:fix"
  },
  "dependencies": {
    "@plausible-analytics/tracker": "^0.4.4",
    "@tailwindcss/vite": "^4.1.11",
    "astro": "^5.12.8",
    "prettier": "^3.5.3",
    "prettier-plugin-fusion": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.11",
    "terser": "^5.44.1",
    "tsx": "^4.0.0",
    "typescript": "^5.9.2"
  }
}
